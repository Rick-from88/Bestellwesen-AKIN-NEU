{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/api/**",
        "run": { "serviceId": "akin-api", "region": "europe-west1" }
      },
      { "source": "**", "destination": "/uebersicht.html" }
    ]
  }
}
